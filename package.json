{
  "name": "@takutakahashi/codex-agentapi",
  "version": "1.0.0",
  "description": "coder/agentapi compatible HTTP API server using @openai/codex-sdk",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "codex-agentapi": "./bin/cli.js"
  },
  "files": [
    "dist",
    "bin",
    ".claude/config.json.example"
  ],
  "scripts": {
    "dev": "bun run src/index.ts",
    "build": "tsc",
    "start": "bun dist/index.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "type-check": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "agentapi",
    "codex",
    "openai",
    "ai-agent",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/takutakahashi/codex-agentapi.git"
  },
  "bugs": {
    "url": "https://github.com/takutakahashi/codex-agentapi/issues"
  },
  "homepage": "https://github.com/takutakahashi/codex-agentapi#readme",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@openai/codex-sdk": "^0.98.0",
    "dotenv": "^16.0.0",
    "express": "^4.18.0",
    "zod": "3.23.8"
  },
  "devDependencies": {
    "@types/express": "^4.17.0",
    "@types/node": "^20.0.0",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitest/coverage-v8": "^2.0.0",
    "eslint": "^8.0.0",
    "supertest": "^7.0.0",
    "typescript": "^5.3.0",
    "vitest": "^2.0.0"
  },
  "engines": {
    "node": ">=18"
  }
}
